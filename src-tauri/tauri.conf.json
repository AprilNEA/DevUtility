{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Developer Utility",
  "version": "0.1.3",
  "identifier": "dev.utility.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "",
        "titleBarStyle": "Overlay",
        "width": 1200,
        "height": 800,
        "transparent": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDJBRDE1RUM3RUY3NUZGMUYKUldRZi8zWHZ4MTdSS3VGSSs2eFA5OVlPcmhtOUJlaWV3UzY2bGFGNzg2TlNyR3lPc2pucng0SE4K",
      "endpoints": [
        "https://cdn.utility.dev/releases/latest/download/latest.json",
        "https://github.com/AprilNEA/DevUtility/releases/latest/download/latest.json"
      ]
    }
  }
}